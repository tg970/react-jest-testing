{
  "name": "react-jest-testing",
  "version": "1.0.0",
  "description": "Example project for testing setup using React, Webpack, and Jest",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest --silent --verbose",
    "start": "node server/index.js",
    "build": "webpack --mode production --json > stats.json",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon server/index.js",
    "dev": "npm run client"
  },
  "jest": {
    "setupFilesAfterEnv": ["<rootDir>src/setupTests.js"],
    "moduleFileExtensions": ["js", "jsx"],
    "modulePaths": ["/src"],
    "moduleDirectories": ["node_modules", "src"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tg970/react-jest-testing.git"
  },
  "author": "Tyler Goodman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tg970/react-jest-testing/issues"
  },
  "homepage": "https://github.com/tg970/react-jest-testing#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@date-io/date-fns": "^2.5.0",
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "base-href-webpack-plugin": "^2.0.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.x.x",
    "css-loader": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.x.x",
    "eslint-import-resolver-webpack": "^0.12.x",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-security": "^1.4.x",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.1.0",
    "lodash.orderby": "^4.6.0",
    "lodash.partition": "^4.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-animate-on-scroll": "^2.1.5",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.8.6",
    "react-google-maps": "^9.4.5",
    "react-hot-loader": "^4.12.12",
    "react-image-gallery": "^1.x.x",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-slick": "^0.25.2",
    "react-swipeable-views": "^0.13.3",
    "react-tagsinput": "^3.19.0",
    "react-test-render": "^1.1.2",
    "react-test-renderer": "^16.13.1",
    "redux": "^4.0.4",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.7.2"
  }
}
